        },
        {
            "displayName": "Odin Examples",
            "description": "A collection of examples using the Odin Inspector",
            "path": "Samples~/Odin Examples"
        },
        {
            "displayName": "PoolManager Examples",
            "description": "A collection of examples using the PoolManager",
            "path": "Samples~/PoolManager Examples"
        },
        {
            "displayName": "UniRx Examples",
            "description": "A collection of examples using the UniRx library",
            "path": "Samples~/UniRx Examples"
        },
        {
            "displayName": "Yaml Examples",
            "description": "A collection of examples using the YamlDotNet library",
            "path": "Samples~/Yaml Examples"
        }
    ]
